{{#if (eq task.state "STARTED")}}
{{#paper-icon task.taskIcon reverseSpin=true class=(get task "taskColor") }}
  {{#paper-tooltip position="right"}}
    {{task.state}}
  {{/paper-tooltip}}
{{/paper-icon}}
{{yield}}
{{else}}
{{#paper-icon task.taskIcon class=task.state}}
  {{#paper-tooltip position="right"}}
    {{task.state}}
  {{/paper-tooltip}}
{{/paper-icon}}
{{yield}}
{{/if}}